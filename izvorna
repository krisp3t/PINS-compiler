fun main(args:arr[10]int):int = (
    # sesteje vsa stevila v argumentih
    {sum = 0},
    {for i = 0, 10, 1:
        {sum = sum + args[i]}
    },
    main(args),
    sum
) {where
    var sum:int;
    var i:int;
    var blabla: string
};

var y: integer;
typ int:integer;
typ str:string;
typ bool:logical;

var x: string
